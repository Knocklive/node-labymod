{
  "name": "node-labymod",
  "version": "2.1.1",
  "description": "A wrapper for the LabyMod-API. Fetch Emotes, Stickers and more!",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "test:typescript": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "prettier": "prettier --write src/**/*.js typings/**/*.ts",
    "prepublishOnly": "npm run test"
  },
  "types": "./typings/index.d.ts",
  "keywords": [
    "labymod",
    "laby",
    "minecraft",
    "api"
  ],
  "author": "masterjanic",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "dtslint": "^4.2.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterjanic/node-labymod.git"
  },
  "bugs": {
    "url": "https://github.com/masterjanic/node-labymod/issues"
  },
  "homepage": "https://github.com/masterjanic/node-labymod#readme",
  "dependencies": {
    "axios": "^0.24.0"
  }
}
