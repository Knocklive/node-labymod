{
  "name": "node-labymod",
  "version": "2.0.0",
  "description": "A wrapper for the LabyMod-API. Fetch Emotes, Stickers and more!",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "test:typescript": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "prettier": "prettier --write src/**/*.js typings/**/*.ts",
    "prepublishOnly": "npm run test"
  },
  "types": "./typings/index.d.ts",
  "keywords": [
    "labymod",
    "laby",
    "minecraft",
    "api"
  ],
  "author": "masterjanic",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dtslint": "^3.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterjanic/node-labymod.git"
  },
  "bugs": {
    "url": "https://github.com/masterjanic/node-labymod/issues"
  },
  "homepage": "https://github.com/masterjanic/node-labymod#readme",
  "dependencies": {
    "axios": "^0.21.1"
  }
}
