{
  "name": "node-labymod",
  "version": "1.1.2",
  "description": "A wrapper for the LabyMod-API. Fetch Emotes, Stickers and more!",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "test:typescript": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "prettier": "prettier --write src/**/*.js typings/**/*.ts",
    "prepublishOnly": "npm run test"
  },
  "types": "./typings/index.d.ts",
  "keywords": [
    "labymod",
    "laby",
    "minecraft",
    "api"
  ],
  "author": "MasterJanic",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all",
    "endOfLine": "lf",
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "dtslint": "^4.0.6",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masterjanic/node-labymod.git"
  },
  "bugs": {
    "url": "https://github.com/masterjanic/node-labymod/issues"
  },
  "homepage": "https://github.com/masterjanic/node-labymod#readme"
}
